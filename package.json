{
  "name": "ezb2",
  "version": "3.0.1",
  "description": "a typed, promise-based api wrapper for backblaze b2.",
  "keywords": [
    "backblaze",
    "b2"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "start": "node dist/main.js",
    "dev": "DEBUG=\"backblaze-b2:*\" DEBUG_HIDE_DATE=true DEBUG_COLORS=true ts-node-dev --transpileOnly --notify=false src/main.ts",
    "build": "rm -rf dist/ && tsc",
    "test": "run-s -l test:*",
    "test:ts": "tsc --noEmit",
    "test:eslint": "eslint 'src/**/*' --ext .ts",
    "test:prettier": "prettier 'src/**/*' -l",
    "prettier": "prettier 'src/**/*' --write",
    "jest": "jest --colors --watch",
    "prepublishOnly": "npm test && npm run build"
  },
  "author": "s <https://github.com/lostfictions>",
  "homepage": "https://github.com/lostfictions/ezb2",
  "bugs": "https://github.com/lostfictions/ezb2/issues",
  "license": "AGPL-3.0",
  "dependencies": {
    "axios": "^0.19.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.14",
    "@typescript-eslint/eslint-plugin": "^2.9.0",
    "@typescript-eslint/parser": "^2.9.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "jest": "^24.9.0",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.19.1",
    "ts-jest": "^24.2.0",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.7.2"
  },
  "engines": {
    "node": ">=10.1.0"
  },
  "jest": {
    "preset": "ts-jest"
  }
}