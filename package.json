{
  "name": "ezb2",
  "version": "2.0.2",
  "description": "a typed, promise-based api wrapper for backblaze b2.",
  "keywords": [
    "backblaze",
    "b2"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "start": "node dist/main.js",
    "dev": "DEBUG=\"backblaze-b2:*\" DEBUG_HIDE_DATE=true DEBUG_COLORS=true ts-node-dev --transpileOnly --notify=false src/main.ts",
    "build": "rm -rf dist/ && tsc",
    "test": "run-s -l test:*",
    "test:ts": "tsc --noEmit",
    "test:eslint": "eslint 'src/**/*'",
    "test:prettier": "prettier 'src/**/*' -l",
    "prettier": "prettier 'src/**/*' --write",
    "jest": "jest --colors --watch",
    "prepublishOnly": "npm test && npm run build"
  },
  "author": "s <https://github.com/lostfictions>",
  "homepage": "https://github.com/lostfictions/ezb2",
  "bugs": "https://github.com/lostfictions/ezb2/issues",
  "license": "AGPL-3.0",
  "dependencies": {
    "axios": "^0.18.0"
  },
  "devDependencies": {
    "@types/jest": "^23.3.7",
    "@types/node": "^10.12.0",
    "eslint": "^5.8.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-typescript": "^0.14.0",
    "jest": "^23.6.0",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.14.3",
    "ts-jest": "^23.10.4",
    "ts-node-dev": "^1.0.0-pre.30",
    "typescript": "^3.2.2",
    "typescript-eslint-parser": "^21.0.2"
  },
  "engines": {
    "node": ">=10.1.0"
  },
  "jest": {
    "preset": "ts-jest"
  }
}
